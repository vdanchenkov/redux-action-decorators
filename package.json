{
  "name": "redux-action-creators",
  "version": "0.0.1",
  "description": "Helpers for Redux action creators",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "coverage": "NODE_PATH='./src' babel-node $(npm bin)/isparta cover --include \"**/!(*test).js\" node_modules/mocha/bin/_mocha -- --recursive src",
    "prepublish": "npm run clean && npm run build",
    "test": "NODE_PATH='./src' mocha --recursive src -w --compilers js:babel-register"
  },
  "author": "Vladimir Danchenkov <vladimir.danchenkov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "redux-actions": "^0.9.1",
    "rimraf": "^2.5.2"
  }
}
